<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#108fd900" />
            <stroke android:width="1.0dip" android:color="#ff449f14" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="#108fd900" />
            <stroke android:width="1.0dip" android:color="#9a449f14" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#108fd900" />
            <stroke android:width="1.0dip" android:color="#67449f14" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
</selector>